 @model PaginatorModel

<section class="pagination d-flex justify-content-center gap-3">
       <a class="@(Model.CurrentPage==1? "disabled ":string.Empty) page-link btn" 
          asp-action="@Model.ActionName" 
          asp-controller="@Model.ControllerName"
          asp-route-pageNumber ="@Model.PrevoiusPage"
          aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
       </a>
       <a class="@(Model.CurrentPage==Model.MaxPagesTotal? "disabled ":string.Empty)page-link btn"
           asp-action="@Model.ActionName" 
           asp-controller="@Model.ControllerName"
           asp-route-pageNumber ="@Model.NextPage"
           aria-label="Next">
           <span aria-hidden="true">&raquo;</span>
        </a>          
</section>